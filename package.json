{
  "name": "usepartyref",
  "version": "1.0.45",
  "license": "MIT",
  "author": "Simon Le Marchant<https://github.com/marchantweb>",
  "homepage": "https://github.com/marchantweb/usePartyRef",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently npm:server:dev npm:dev:playground",
    "dev:playground": "vite --host --open",
    "build": "vite build",
    "preview": "vite preview",
    "build:lib": "vite build --mode library",
    "publish:lib": "npm run build:lib && npm publish --access public",
    "server:dev": "partykit dev --live",
    "server:deploy": "partykit deploy"
  },
  "dependencies": {
    "partykit": "^0.0.107",
    "partysocket": "^1.0.1",
    "rate-limiter-flexible": "^5.0.3",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.21"
  }
}
